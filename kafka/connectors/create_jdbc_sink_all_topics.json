{
  "connector.class"                    : "io.confluent.connect.jdbc.JdbcSinkConnector",
  "connection.url"                     : "jdbc:postgresql://localhost:5432/kafkacdb",
  "topics"                             : "component_topic, part_topic, assembly_topic",
  "table.name.format"                  : "T_FACTORY",
  "key.converter"                      : "org.apache.kafka.connect.json.JsonConverter",
  "key.converter.schemas.enable"       : "true",
  "value.converter"                    : "org.apache.kafka.connect.json.JsonConverter",
  "value.converter.schemas.enable"     : "true",
  "connection.user"                    : "kafkac-admin",
  "connection.password"                : "kafkac-admin",
  "insert.mode"                        : "upsert",
  "auto.create"                        : true,
  "auto.evolve"                        : true,
  "transforms"                         : "InsertField",
  "transforms.InsertField.type"        : "org.apache.kafka.connect.transforms.InsertField$Value",
  "transforms.InsertField.static.field": "kafka_status",
  "transforms.InsertField.static.value": "STREAMED",
  "pk.mode"                            : "record_key",
  "pk.fields"                          : "id",
  "errors.tolerance"                   : "all",
  "errors.log.enable"                  : "true",
  "tasks.max"                          : "1",
  "delete.enabled"                     : "false"
}
