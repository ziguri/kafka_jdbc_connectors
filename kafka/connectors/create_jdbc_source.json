{
  "connector.class"                    : "io.confluent.connect.jdbc.JdbcSourceConnector",
  "dialect.name"                       : "PostgreSqlDatabaseDialect",
  "connection.url"                     : "jdbc:postgresql://postgres:5432/kafkacdb",
  "key.converter"                      : "org.apache.kafka.connect.json.JsonConverter",
  "key.converter.schemas.enable"       : "true",
  "value.converter"                    : "org.apache.kafka.connect.json.JsonConverter",
  "value.converter.schemas.enable"     : "true",
  "topic.prefix"                       : "",
  "query"                              : "select a.id, a.factory_info, a.kafka_topic, a.kafka_status from T_FACTORY a",
  "connection.user"                    : "kafkac-admin",
  "connection.password"                : "kafkac-admin",
  "incrementing.column.name"           : "id",
  "tasks.max"                          : "1",
  "mode"                               : "incrementing",
  "transforms"                         : "createKey, extractInt, replaceField, topicName",
  "transforms.topicName.type"          : "io.confluent.connect.transforms.ExtractTopic$Value",
  "transforms.topicName.field"         : "kafka_topic",
  "transforms.createKey.type"          : "org.apache.kafka.connect.transforms.ValueToKey",
  "transforms.createKey.fields"        : "id",
  "transforms.extractInt.type"         : "org.apache.kafka.connect.transforms.ExtractField$Key",
  "transforms.extractInt.field"        : "id",
  "transforms.replaceField.type"       : "org.apache.kafka.connect.transforms.ReplaceField$Value",
  "transforms.replaceField.blacklist"  : "kafka_status",
  "table.poll.interval.ms"             : "5000",
  "errors.tolerance"                   : "all",
  "errors.log.enable"                  : "true"
}
